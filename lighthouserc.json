{
    "ci": {
      "collect": {
        "staticDistDir": "./_site",
        "url": ["/index.html"]
      },
      "assert": {
        "preset": "lighthouse:no-pwa",
        "assertions": {
          "is-crawlable": false,
          "errors-in-console": ["error", { "maxLength": 1 }],
          "unused-css-rules": "off"
        }
      }
    }
  }
  